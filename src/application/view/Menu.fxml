<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="625.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.291" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MenuController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Canvas fx:id="canvas" height="600.0" width="800.0" />
            <Label fx:id="startLabel" alignment="CENTER" layoutX="300.0" prefHeight="100.0" prefWidth="200.0" text="Start" textAlignment="CENTER">
               <font>
                  <Font size="45.0" />
               </font>
            </Label>
            <Label fx:id="quitLabel" alignment="CENTER" layoutX="300.0" layoutY="500.0" prefHeight="100.0" prefWidth="200.0" text="Quit" textAlignment="CENTER">
               <font>
                  <Font size="45.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
